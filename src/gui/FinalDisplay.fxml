   <children>
      <Label layoutX="64.0" layoutY="53.0" text="Final Results" />
      <TextArea fx:id="resultBox" layoutX="64.0" layoutY="77.0" prefHeight="200.0" prefWidth="472.0" />
      <Button fx:id="backBtn" layoutX="64.0" layoutY="320.0" mnemonicParsing="false" onAction="#handleBackBtnAction" text="Go Back to Main Menu" />
      <Button fx:id="quitBtn" layoutX="278.0" layoutY="320.0" mnemonicParsing="false" onAction="#handleQuitButtonAction" text="Quit" />
   </children>
</AnchorPane>
